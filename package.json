{
  "name": "new",
  "scripts": {
    "build": "rimraf dist && swc src --config-file .swcrc.json --out-dir dist --ignore **/*.test.ts",
    "build:watch": "yarn build -w",
    "clean": "rimraf dist",
    "deploy": "yarn build && (yarn deploy:app)",
    "deploy:app": "python ./scripts/cloud_run.py",
    "dev": "yarn serve",
    "lint": "eslint src --ext ts --cache",
    "prepare": "husky install",
    "serve": "nodemon",
    "start": "node -r ./dotenv-preload.js -r ./tsconfig-paths-bootstrap.js dist/index.js",
    "test": "jest",
    "type": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.29",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.20.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "redis": "^4.6.7"
  }
}
